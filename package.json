{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dcc": "docker compose -f cicd/dev/docker-compose.yml --env-file .env",
    "db-push": "npx drizzle-kit push --config=db/drizzle.config.ts",
    "db-seed": "tsx ./db/seed.ts",
    "db-generate": "npx drizzle-kit generate --config=db/drizzle.config.ts",
    "db-migrate": "npx drizzle-kit migrate --config=db/drizzle.config.ts"
  },
  "dependencies": {
    "@nuxt/eslint": "^0.6.1",
    "@nuxt/fonts": "^0.10.2",
    "@primevue/themes": "^4.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "firebase": "^11.0.1",
    "firebase-admin": "^12.7.0",
    "nuxt": "^3.13.2",
    "nuxt-vuefire": "^1.0.4",
    "pg": "^8.13.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.1.1",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@primevue/nuxt-module": "^4.1.1",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.26.2",
    "tsx": "^4.19.1"
  }
}
